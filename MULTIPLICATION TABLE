public class Main
{
    /*
    aweirdgift
    MULTIPLICATION TABLE
    */
    
    
	public static void main(String[] args) {
	
	for (int i = 1; i <=10; i++ ) {
	    for (int x = 0; x <=10; x++) {
	        System.out.println(i + " x " + x + " = " + i * x );
	    }
	    System.out.println(" \n  \n");
	  }
	}
}
